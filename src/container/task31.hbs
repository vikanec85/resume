<main>
    {{#with navigation}}
    <div class="row ">
        <section class="col-12">

            <div class="list-group list-group-flush">
                <div class="list-group-item ">
                    {{#each links}}
                    <a href="{{href}}" class="btn btn-link">{{text}}</a>
                    {{/each}}
                    {{#with button}}
                    <a class="btn btn-primary" href="{{href}}" role="button">{{text}}</a>
                    {{/with}}
                </div>
            </div>

        </section>
    </div>
    {{/with}}
    <hr>
    <section>
        {{#with header}}
        <h1>{{title}}</h1>
        <p>{{ description}}</p>
        {{#with button}}
        <button href="{{link}}">{{text}}</button>
        {{/with}}
        {{/with}}
    </section>
    <hr>
    {{#with city}}
    <div class="row ">
        <h2>{{title}}</h2>
        {{#each cards}}
        <div class="col-5">
            <div class="card">

                {{#with picture}}
                <img src="{{src}}" class="card-img-top" alt="{{alt}}">
                {{/with}}
                <div class="card-body">
                    <h5 class="card-title">{{title}}</h5>
                    <p class="card-text">{{description}}</p>
                    {{#with button}}
                    <button href="{{href}}" class="btn btn-primary">{{text}}</button>
                    {{/with}}
                </div>

            </div>
        </div>
        {{/each}}
    </div>
    {{/with}}
    <hr>
    <section>
        {{#with winner}}
        <h2>{{title}}</h2>
        {{#with table}}
        <table class="table ">

            <tr>
                <th>
                    {{head}}
                </th>
            </tr>

            {{#each user}}
            <tr>
                <th>{{place}}</th>
                <td>{{name}}</td>
                <td>{{surname}}</td>
                <td>{{nickname}}</td>
            </tr>
            {{/each}}

        </table>
        {{/with}}
        {{/with}}
    </section>
    <hr>

    <section>
        {{#with gallery}}
        <h2>{{title}}</h2>
        <div class="row ">
            {{#each cards}}
            <div class="col-4">
                <div class="card ">

                    <div class="card-header">
                        <div>{{name}}</div>
                    </div>
                    <ul class="list-group list-group-flush">
                        <li class="list-group-item  ">
                            {{items}}
                        </li>
                    </ul>

                </div>
            </div>
            {{/each}}
        </div>
        {{/with}}
    </section>

    <hr>
    <section>
        {{#with relaxion}}
        <h1 class="display-3">{{title}}</h1>
        <div class="row">
            {{#each cards}}
            <div class="col-6">
                <div class="card text-bg-secondary mb-3" style="max-width: 18rem;">
                    <div class="card-header">{{header}}</div>
                    <div class="card-body">
                        <h5 class="card-title">{{title}}</h5>
                        <p class="card-text">{{description}}</p>
                    </div>
                </div>
            </div>
            {{/each}}
        </div>
        {{/with}}
    </section>

    <hr>
    <section>
        {{#with rest}}
        <div class="row">
            {{#each cards}}
            <div class="col-6">
                <div class="col-6"></div>
                <div class="card text-center">
                    <div class="card-header">
                        {{header}}
                    </div>
                    <div class="card-body">
                        <h5 class="card-title">{{title}}</h5>
                        <p class="card-text">{{description}}</p>
                        <a href="#" class="btn btn-primary">{{button}}</a>
                    </div>
                    <div class="card-footer text-body-secondary">
                        {{footer}}
                    </div>
                </div>
            </div>
            {{/each}}
        </div>
        {{/with}}
    </section>

    <hr>
    <section>
        {{#with proposition}}
        <div class="row">
            <h1 class="display-2">{{title}}</h1>
            {{#each card}}
            <div class="col-6">
                <div class="card text-bg-secondary mb-3" style="max-width: 18rem;">
                    <div class="card-header">{{header}}</div>
                    <div class="card-body">
                        <h5 class="card-title">{{title}}</h5>
                        <p class="card-text">{{description}}</p>
                    </div>
                </div>
            </div>
            {{/each}}
        </div>
        {{/with}}
    </section>

    <hr>
    <section>
        {{#with footer}}
        <h5>{{title}}</h5>
        <div class="row">

            {{#each navigation}}
            <div class="col-4">
                <a class="btn btn-outline-primary" href="{{href}}" role="button">{{text}}</a>
            </div>
            {{/each}}
            {{#each socialMedia}}
            <div class="col-4">
                <a class="btn btn-outline-dark" href="{{link}}">{{name}}</a>
            </div>
            {{/each}}
            {{#each partners}}
            <div class="col-4">
                <a class="btn btn-outline-info" href="{{link}}">{{name}}</a>
            </div>
            {{/each}}

        </div>
        {{/with}}
    </section>
</main>